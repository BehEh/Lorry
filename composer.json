	"type": "project",
	"license": "GPL-3.0",
	"require": {
		"php": ">= 5.6",
		"ext-json": "*",
		"ext-gettext": "*",
		"ext-pdo": "*",
		"ext-openssl": "*",
		"ext-curl": "*",
		"twig/twig": "~1.16",
		"twig/extensions": "~1.2",
		"swiftmailer/swiftmailer": "~5.3",
		"lightopenid/lightopenid": "~0.7",
		"league/oauth2-client": "0.6.0",
		"analog/analog": "~1.0.4-stable",
		"symfony/yaml": "~2.5",
		"symfony/finder": "~2.5",
        "chrisboulton/php-resque": "dev-unixsocket",
		"aws/aws-sdk-php": "2.*"
    },
	"suggest": {
		"ext-http": "More reliable language negotiation"
	},
	"repositories": [
        {
            "type": "vcs",
